<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/nav_home">

    <fragment
        android:id="@+id/nav_home"
        android:name="com.example.kaisersentinel.ui.home.Home_Fragment"
        android:label="Home"
        tools:layout="@layout/fragment_home" />

    <fragment
        android:id="@+id/nav_notification"
        android:name="com.example.kaisersentinel.ui.notification.Notification_Fragment"
        android:label="Notification" />

    <fragment
        android:id="@+id/nav_report"
        android:name="com.example.kaisersentinel.ui.report.Report_Fragment"
        android:label="Report"
        tools:layout="@layout/fragment_report" />

    <fragment
        android:id="@+id/nav_marked"
        android:name="com.example.kaisersentinel.ui.marked.Marked_Fragment"
        android:label="Marked"
        tools:layout="@layout/fragment_marked" />

    <fragment
        android:id="@+id/nav_configure"
        android:name="com.example.kaisersentinel.ui.configure.Configure_Fragment"
        android:label="Configure"
        tools:layout="@layout/fragment_configure" />

    <fragment
        android:id="@+id/nav_logout"
        android:name="com.example.kaisersentinel.ui.logout.LogoutFragment"
        android:label="Logout"
        tools:layout="@layout/fragment_logout" />
</navigation>